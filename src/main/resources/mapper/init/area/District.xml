<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace必须指向Dao接口 -->
<mapper namespace="net.leebao.busi.dao.init.area.DistrictMybatisDao">
    <select id="findDistrictsByCityCode" resultType="net.leebao.busi.entity.init.area.District" parameterType="string">
        select d.code,d.name,d.city_code
        from t_bi_district d
        where d.city_code=#{0}
    </select>
</mapper>