<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace必须指向Dao接口 -->
<mapper namespace="net.leebao.open.busi.dao.init.area.TownMybatisDao">
    <select id="findTownsByDistrictCode" resultType="net.leebao.open.busi.entity.init.area.Town" parameterType="string">
        select t.code,t.name,t.district_code
        from t_bi_town t
        where t.district_code=#{0}
    </select>
</mapper>